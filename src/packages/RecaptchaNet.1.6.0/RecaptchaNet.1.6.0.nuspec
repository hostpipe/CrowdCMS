    <id>RecaptchaNet</id>
    <version>1.6.0</version>
    <title>Recaptcha for .NET</title>
    <authors>Tanveer Yousuf</authors>
    <owners>Tanveer Yousuf</owners>
    <licenseUrl>http://recaptchanet.codeplex.com/license</licenseUrl>
