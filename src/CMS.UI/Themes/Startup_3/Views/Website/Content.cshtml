    @{ Html.RenderPartial("/Views/Partials/MetaTags.cshtml", new CMS.UI.Models.MetadataModel(Model.Content));}
}
@section styles {
    @Html.Raw(Model != null ? Model.CSS : String.Empty)
}
@section scripts {

}
@section sidebar {
    @{
        Html.RenderPartial("../Partials/SearchPanel");
